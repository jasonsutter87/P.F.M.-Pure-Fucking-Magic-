{
  "name": "get-pfm",
  "version": "0.1.2",
  "description": "PFM parser for JavaScript/TypeScript. Read and validate .pfm AI agent output files.",
  "type": "module",
  "main": "./dist/cjs/index.cjs",
  "module": "./dist/esm/index.js",
  "types": "./dist/esm/index.d.ts",
  "exports": {
    ".": {
      "import": {
        "types": "./dist/esm/index.d.ts",
        "default": "./dist/esm/index.js"
      },
      "require": {
        "types": "./dist/cjs/index.d.cts",
        "default": "./dist/cjs/index.cjs"
      }
    }
  },
  "bin": {
    "pfm": "./dist/esm/cli.js",
    "get-pfm": "./dist/esm/cli.js"
  },
  "files": [
    "dist",
    "README.md",
    "LICENSE"
  ],
  "scripts": {
    "build": "npm run build:esm && npm run build:cjs",
    "build:esm": "tsc -p tsconfig.esm.json",
    "build:cjs": "tsc -p tsconfig.cjs.json && node scripts/fixup-cjs.js",
    "test": "node --test dist/esm/__tests__/index.test.js dist/esm/__tests__/conformance.test.js",
    "prepublishOnly": "npm run build && npm test"
  },
  "keywords": [
    "pfm",
    "parser",
    "ai",
    "agent",
    "file-format",
    "container",
    "llm"
  ],
  "author": "Jason Sutter",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/jasonsutter87/P.F.M..git"
  },
  "devDependencies": {
    "@types/node": "^20.19.33",
    "typescript": "^5.3.0"
  },
  "engines": {
    "node": ">=18.0.0"
  }
}
